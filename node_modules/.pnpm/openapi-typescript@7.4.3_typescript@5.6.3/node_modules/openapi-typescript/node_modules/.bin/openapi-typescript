#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/iraklitbz/Desktop/apps/sabania/node_modules/.pnpm/openapi-typescript@7.4.3_typescript@5.6.3/node_modules/openapi-typescript/bin/node_modules:/Users/iraklitbz/Desktop/apps/sabania/node_modules/.pnpm/openapi-typescript@7.4.3_typescript@5.6.3/node_modules/openapi-typescript/node_modules:/Users/iraklitbz/Desktop/apps/sabania/node_modules/.pnpm/openapi-typescript@7.4.3_typescript@5.6.3/node_modules:/Users/iraklitbz/Desktop/apps/sabania/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/iraklitbz/Desktop/apps/sabania/node_modules/.pnpm/openapi-typescript@7.4.3_typescript@5.6.3/node_modules/openapi-typescript/bin/node_modules:/Users/iraklitbz/Desktop/apps/sabania/node_modules/.pnpm/openapi-typescript@7.4.3_typescript@5.6.3/node_modules/openapi-typescript/node_modules:/Users/iraklitbz/Desktop/apps/sabania/node_modules/.pnpm/openapi-typescript@7.4.3_typescript@5.6.3/node_modules:/Users/iraklitbz/Desktop/apps/sabania/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
