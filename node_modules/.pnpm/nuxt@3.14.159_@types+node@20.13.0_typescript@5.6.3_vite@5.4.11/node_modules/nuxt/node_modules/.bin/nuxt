#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/iraklitbz/Desktop/apps/sabania/node_modules/.pnpm/nuxt@3.14.159_@types+node@20.13.0_typescript@5.6.3_vite@5.4.11/node_modules/nuxt/bin/node_modules:/Users/iraklitbz/Desktop/apps/sabania/node_modules/.pnpm/nuxt@3.14.159_@types+node@20.13.0_typescript@5.6.3_vite@5.4.11/node_modules/nuxt/node_modules:/Users/iraklitbz/Desktop/apps/sabania/node_modules/.pnpm/nuxt@3.14.159_@types+node@20.13.0_typescript@5.6.3_vite@5.4.11/node_modules:/Users/iraklitbz/Desktop/apps/sabania/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/iraklitbz/Desktop/apps/sabania/node_modules/.pnpm/nuxt@3.14.159_@types+node@20.13.0_typescript@5.6.3_vite@5.4.11/node_modules/nuxt/bin/node_modules:/Users/iraklitbz/Desktop/apps/sabania/node_modules/.pnpm/nuxt@3.14.159_@types+node@20.13.0_typescript@5.6.3_vite@5.4.11/node_modules/nuxt/node_modules:/Users/iraklitbz/Desktop/apps/sabania/node_modules/.pnpm/nuxt@3.14.159_@types+node@20.13.0_typescript@5.6.3_vite@5.4.11/node_modules:/Users/iraklitbz/Desktop/apps/sabania/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../../bin/nuxt.mjs" "$@"
fi
